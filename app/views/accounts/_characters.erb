<% account = characters %>
<tr>
    <td><%= link_to(account.name, account_url(account)) %></td>
    <% account.active_characters.each do |character| %>
        <td>
            <%= link_to(render(:partial => "shared/character",
                               :object => character),
                        "#{account_url(account)}##{dom_id(character)}") %>
            &nbsp;
            <%= character.level %>
        </td>
    <% end %>
</tr>
