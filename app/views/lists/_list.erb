<div class="raid">
  <h3><%= list.name %></h3>
  <dl>
    <% list.list_positions.each do |lp| %>
      <dt>
        <%= link_to(lp.account.name,
                    account_url(lp.account)) %>
        <%= lp.position %>
      </dt>
      <% lp.account.characters.each do |character| %>
        <dd>
          <%= link_to(render(:partial => "shared/character", :object => character),
                      "#{account_url(lp.account)}#character_#{character.id}") %>
        </dd>
      <% end %>
    <% end %>
  </dl>
</div>
